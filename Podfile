source 'https://github.com/CocoaPods/Specs.git'
platform :ios, "7.0"

target 'CodeShare' do

pod 'AFNetworking'


# AutoLayout
pod 'Masonry'

# Mob产品公共库
pod 'MOBFoundation_IDFA'
# SMSSDK必须
pod 'SMSSDK'

# GitHub 官方出品的一个响应式编程框架，2.0版本是用OC实现，3.0以后是swift
pod 'ReactiveCocoa','~> 2.0'


pod 'YYModel'
pod 'YYImage'
pod 'YYWebImage'

pod 'OpenShare'

pod 'QR-Code-Encoder-for-Objective-C'
end
